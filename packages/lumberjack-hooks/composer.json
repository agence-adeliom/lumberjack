{
    "name": "agence-adeliom/lumberjack-hooks",
    "require": {
        "php": ">=8.0.2",
        "doctrine/annotations": "^1.6",
        "rareloop/lumberjack-core": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Hooks\\": "src"
        },
        "files": [
            "src/Helpers/Utils.php",
            "src/Helpers/RegisterAnnotations.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Hooks\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
