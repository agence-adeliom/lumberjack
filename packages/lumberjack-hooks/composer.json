{
    "name": "agence-adeliom/lumberjack-hooks",
    "keywords": ["hooks", "wordpress", "timber", "lumberjack", "php-attributes"],
    "license": "MIT",
    "require": {
        "php": ">=8.0.2",
        "doctrine/annotations": "^1.6",
        "rareloop/lumberjack-core": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Hooks\\": "src"
        },
        "files": [
            "src/Helpers/Utils.php",
            "src/Helpers/RegisterAnnotations.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "conflict": {
        "agence-adeliom/lumberjack-cron": "<1.0.1",
        "agence-adeliom/lumberjack-blocks": "<1.0.1",
        "agence-adeliom/lumberjack-pagination": "<1.0.1",
        "agence-adeliom/lumberjack-taxonomy": "<1.0.1",
        "agence-adeliom/lumberjack-admin": "<1.0.1",
        "agence-adeliom/lumberjack-assets": "<1.0.1"
    }
}
