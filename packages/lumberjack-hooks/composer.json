{
    "name": "agence-adeliom/lumberjack-hooks",
    "require": {
        "php": "^7.2||^8.0",
        "doctrine/annotations": "^1.6"
    },
    "require-dev": {
        "rareloop/lumberjack-core": "^4.0||^5.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Hooks\\": "src"
        },
        "files": [
            "src/Helpers/Utils.php",
            "src/Helpers/RegisterAnnotations.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Hooks\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
