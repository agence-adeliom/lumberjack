{
    "name": "agence-adeliom/lumberjack-events",
    "require": {
        "php": ">=8.0.2",
        "brick/event": "^0.1.1",
        "rareloop/lumberjack-core": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Event\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adeliom\\Lumberjack\\Event\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
